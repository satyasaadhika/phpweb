{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/assets/check.php",
        "dest": "/public/assets/check.php"
      },
      {
        "src": "/assets/fail.php",
        "dest": "/public/assets/fail.php"
      },
      {
        "src": "/assets/datas/data.json",
        "dest": "/public/assets/datas/data.json"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}